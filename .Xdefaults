URxvt.saveLines:    10000
URxvt.scrollBar:    false
URxvt.foreground:   grey
URxvt.background:   black
URxvt.font: xft:montecarlo:pixelsize=8:antialias=false
URxvt.italicFont: xft:Consolas:pixelsize=9:regular:italic:autohint=true:antialias=true
Xft*dpi:              96
Xft*antialias:        False
Xft*hinting:          None
XTerm*termName: xterm-256color
URxvt*termName: rxvt-unicode

URxvt.perl-ext:      default,keyboard-select,clipboard,url-select
URxvt.keysym.Shift-Control-U:    perl:url-select:select_next

URxvt.keysym.Shift-Control-F: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste


